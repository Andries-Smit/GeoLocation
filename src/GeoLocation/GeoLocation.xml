<?xml version="1.0" encoding="utf-8" ?>
<widget id="GeoLocation.widget.GeoLocation" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>GeoLocation</name>
    <description>The description of this widget.</description>

    <icon>
        
    </icon>

    <properties>
        <property key="url" type="attribute" required="true">
            <caption>Microflow service URL</caption>
            <category>Behavior</category>
            <description>The URL of the microflow service that handles sent locations.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>     
        <property key="debug" type="attribute" required="true">
            <caption>Debug</caption>
            <category>Behavior</category>
            <description>When enabled, the plugin will emit sounds for life-cycle events of background-geolocation.</description>
            <attributeTypes>
                <attributeType name="Boolean"/>
            </attributeTypes>
        </property>
        <property key="desiredAccuracy" type="attribute" required="true">
            <caption>Desired accuracy</caption>
            <category>Behavior</category>
            <description>Desired accuracy in meters. Possible values [0, 10, 100, 1000]. Lower is better.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>      
        <property key="notificationTitle" type="string" required="true" defaultValue="GPS Tracking">
            <caption>Notification title</caption>
            <category>Notifications</category>
            <description>Custom notification title in the drawer.</description>
        </property>
        <property key="notificationText" type="string" required="true" defaultValue="ENABLED">
            <caption>Notification text</caption>
            <category>Notifications</category>
            <description>Custom notification text in the drawer.</description>
        </property>
        <property key="interval" type="attribute" required="true">
            <caption>Minimum interval</caption>
            <category>Behavior</category>
            <description>The minimum time interval between location updates in milliseconds.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
        <property key="fastestInterval" type="attribute" required="true">
            <caption>Fastest interval</caption>
            <category>Behavior</category>
            <description>Fastest rate in milliseconds at which your app can handle location updates.</description>
            <attributeTypes>
                <attributeType name="Integer"/>
            </attributeTypes>
        </property>
    </properties>
</widget>

